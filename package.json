{
  "name": "teleport-lib-js",
  "version": "0.1.0",
  "description": "",
  "main": "dist/index.js",
  "watch": {
    "test:coverage": {
      "patterns": [
        "src"
      ],
      "extensions": "ts,json,js",
      "quiet": true,
      "legacyWatch": true
    }
  },
  "nyc": {
    "exclude": [
      "dist",
      "src/__test__"
    ]
  },
  "scripts": {
    "postinstall": "typings install dt~mocha dt~require dt~chai --save --global",
    "watch-build": "./node_modules/.bin/tsc -w",
    "build": "npm run clean && ./node_modules/.bin/tsc",
    "build-docs": "rimraf docs && typedoc --options typedoc.json src/",
    "clean": "rm -rf dist",
    "test": "mocha --colors --require ts-node/register src/**/*.spec.ts",
    "test:coverage": "nyc -e '.ts' --r html -r lcov -r text npm run test",
    "watch-test": "npm-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/teleporthq/teleport-lib-js.git"
  },
  "author": "Cristian Moldovan",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^5.0.0",
    "chai": "^4.1.2",
    "istanbul": "^0.4.5",
    "mocha": "^5.0.5",
    "nock": "^9.2.3",
    "nyc": "^11.6.0",
    "rimraf": "^2.6.2",
    "source-map-support": "^0.5.4",
    "ts-node": "^5.0.1",
    "typedoc": "^0.11.1",
    "typescript": "^2.7.2",
    "typings": "^2.1.1"
  },
  "dependencies": {
    "@types/node": "^9.6.0",
    "bluebird": "^3.5.1",
    "is-url": "^1.2.3",
    "isomorphic-fetch": "^2.2.1",
    "jss": "^9.8.1",
    "jss-preset-default": "^4.3.0",
    "lodash": "^4.17.5",
    "node-fetch": "^2.1.1",
    "npm-watch": "^0.3.0",
    "tslint": "^5.9.1"
  }
}
